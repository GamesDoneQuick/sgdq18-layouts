{
	"extends": "tslint-xo",
	"rules": {
		"comment-format": false,
		"early-exit": false,
		"no-require-imports": false,
		"no-single-line-block-comment": false,
		"no-unused-variable": [true, {"ignore-pattern": "^_"}],
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
	}
}
